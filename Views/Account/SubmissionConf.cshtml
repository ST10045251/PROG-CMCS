@model ST10045251_PROTOTYPE.Models.Claim

<h2>Submission Confirmation</h2>
<p>Your claim has been submitted successfully.</p>
<ul>
    <li>Claim ID: @Model.ClaimId</li>
    <li>Claimant Name: @Model.ClientName</li>
    <li>Amount: @Model.Amount</li>
    <li>Status: @Model.Status</li>
    <li>Submitted On: @Model.SubmissionDate</li>
</ul>
<a asp-action="Submit">Submit Another Claim</a>
